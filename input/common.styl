@import "reset.css";

.fixed-menu
    position fixed
    top 0
    left 0
    right 0
    background #ffffff
    z-index 1
    border-bottom 1px solid #eaeaea
    z-index 10

.public-header
    position relative
    height 100px
    @media(max-width 767px)
        height 60px
    .brand
        height 100%
        font-size 0
        i
            display inline-block
            width 0
            height 100%
        img
        i
            vertical-align middle
        img
            @media(max-width 767px)
                height 40px
    .clear
        height 100%
        @media(max-width 1199px)
            width 100%
            padding 20px 0 30px
            display flex
            justify-content center
            align-items center
            flex-direction column
            height auto 
    .mobile-menu
        height 100%
        left 0
        right 0
        width 100%
        background #ffffff
        z-index 10
        @media(max-width 1199px)
            position absolute
            height auto
            left -15px
            right -15px
            padding 0 15px
            display none
    .moblie-toggle
        height 100%
        width 50px
        font-size 0
        text-align center
        display none
        cursor pointer
        @media(max-width 1199px)
            display block
        @media(max-width 767px)
            padding 0 10px
            box-sizing border-box
        i
        span
            display inline-block
            vertical-align middle
        &.active
            span
                background rgba(0,0,0,0)
                &:after
                    top 0
                    transform rotateZ(45deg)
                    transition top 0.3s,bottom 0.3s,transform 0.3s 0.3s
                &:before
                    bottom 0
                    transform rotateZ(-45deg)
                    transition top 0.3s,bottom 0.3s,transform 0.3s 0.3s
        span
            position relative
            width 100%
            height 2px
            background #856b36
            cursor pointer
            transition 0.3s
            &:before
            &:after
                content ''
                position absolute
                left 0
                right 0
                height 2px
                width 100%
                background #856b36
                transition 0.3s
            &:after
                top -20px
                transition top 0.3s 0.3s,bottom 0.3s 0.3s,transform 0.3s
                @media(max-width 767px)
                    top -13px
            &:before
                bottom -20px
                transition top 0.3s 0.3s,bottom 0.3s 0.3s,transform 0.3s
                @media(max-width 767px)
                    bottom -13px
                
        i
            width 0
            height 100%
    .menu
        height 100%
        .nav
            font-size 18px
            color #1d2741
            line-height 100px
            @media(max-width 1199px)
                float none
                line-height 50px
            .item
                position relative
                @media(max-width 1199px)
                    float none
            a
                display block
                padding 0 20px
                transition 0.3s
                &:hover
                    color #ffffff
                    background #ac915b
                &.active
                    color #ac915b
                &.on
                    .iconfont
                        display inline-block
                        transform rotate(180deg)
            .child-menu
                display none
                position absolute
                top 100px
                left 0
                right 0
                background #ffffff
                z-index 10
                @media(max-width 1199px)
                    position static
                    padding 0 15px
                .item
                    height 45px
                    width 100%
                    a
                        font-size 14px
                        padding 0 10px
                        line-height 45px
        .login-other
            padding-left 50px
            font-size 0
            height 100%
            @media(max-width 1199px)
                padding-left 0
                padding-right 30px
                padding 0
            a
            i
                display inline-block
                vertical-align middle
            i
                width 0
                height 100%
            a
                margin-left 10px
                width 28px
                height 28px
                border-radius 50%
                text-align center
                line-height 28px
                font-size 18px
                color #ffffff
                background #cccccc
                transition 0.3s
                @media(max-width 1199px)
                    margin 0 20px
                &:hover
                    background #ac915b
        .login-register
            margin-left 20px
            font-size 0
            height 100%
            text-align right
            position relative
            @media(max-width 1199px)
                margin-left 0
                margin-top 20px
            &:after
                content ''
                position absolute
                left 50%
                height 14px
                top 50%
                margin-top -7px
                background #ded3bd
                width 1px
            a
            i
                display inline-block
                vertical-align middle
            i
                width 0
                height 100%
            a
                font-size 16px
                color #333333
                transition 0.3s
                @media(max-width 1199px)
                    margin 0 20px
                &:hover
                    color #ac915b
                &+a
                    margin-left 30px



